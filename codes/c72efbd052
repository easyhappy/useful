            jQuery(document).ready(function($) {

                var info_download   = $('#info_download'),
                    info_content    = $('.content', info_download),
                    close_button    = $('<button class="close">&times;</button>'),
                    input_name      = $('#input_name'),
                    input_comment   = $('#input_comment'),
                    url             = '/user12143456/review/69109140/c72efbd052';

                Player.init.loader();

                $('#info_toggle').click(function() {
                    info_content.toggle();
                    return;
                });

                close_button.appendTo(info_content).click(function() {
                    info_content.hide();
                    return false;
                });

                $('a.delete', '#comments').live('click', function() {

                    var link = $(this),
                        clip_id     = link.attr('data-clip-id'),
                        comment_id  = link.attr('data-comment-id');

                    if (confirm('Are you sure you wish to delete this comment?')) {
                        $.cookie('xsrft', XSRFT, {
                            domain: '.vimeo.com',
                            path: '/'
                        });

                        $.post(url, {
                            action: 'delete_comment',
                            comment_id:    comment_id,
                            token:      XSRFT
                        }, function() {
                            link.parents("article").fadeOut();
                        });
                    }

                    return false;

                });

                $('#add_comment').submit(function() {

                    var form_is_valid = true;

                    // Reset state on every check (gives user benefit of the doubt)
                    if (input_name.length > 0) {
                        input_name.removeClass('invalid');
                    }

                    if (input_comment.length > 0) {
                        input_comment.removeClass('invalid');
                    }

                    // Check validity of inputs
                    if (input_name.length > 0 && input_name.val() === '') {
                        form_is_valid = false;
                        input_name.addClass('invalid');
                    }

                    if (input_comment.length > 0 && input_comment.val() === '') {
                        form_is_valid = false;
                        input_comment.addClass('invalid');
                    }

                    if (form_is_valid) {
                        $.cookie('xsrft', XSRFT, {
                            domain: '.vimeo.com',
                            path: '/'
                        });

                        $.post(url, {
                            action:     'add_comment',
                            name:       input_name.val(),
                            comment:    input_comment.val(),
                            token:      XSRFT
                        }, function(html) {
                            $('#comments').html(html);
                            input_name.val('');
                            input_comment.val('');
                        });
                    }

                    return false;

                });

            });
        </script>
